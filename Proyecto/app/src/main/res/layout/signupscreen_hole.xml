<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id123"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/signup_hole_des_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Descripción"/>

    <EditText
        android:id="@+id/signup_hole_des_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Descripción"
        android:inputType="text"
        tools:ignore="Autofill"/>

    <TextView
        android:id="@+id/signup_hole_metros_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Metros"/>

    <EditText
        android:id="@+id/signup_hole_metros_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Metros"
        android:inputType="number"
        tools:ignore="Autofill"/>


    <TextView
        android:id="@+id/signup_hole_par_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Par"/>

    <EditText
        android:id="@+id/signup_hole_par_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Par"
        android:inputType="number"
        tools:ignore="Autofill"/>




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/crear_hole_button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/crear"
            tools:ignore="ButtonStyle" />

        <Button
            android:id="@+id/cancelar_hole_button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/signup_cancelar"
            tools:ignore="ButtonStyle" />
    </LinearLayout>

</LinearLayout>